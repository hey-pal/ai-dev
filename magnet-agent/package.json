{
  "name": "magnet-agent",
  "version": "0.1.25",
  "description": "AI coding assistant that uses tools to perform tasks on your codebase.",
  "keywords": [
    "ai",
    "gpt",
    "openai",
    "langchain",
    "agent",
    "code",
    "assistant"
  ],
  "author": "Toolkit AI <team@toolkit.ai>",
  "homepage": "https://toolkitai.notion.site/Magnet-Docs-55cd2321039443d695235cadb884cabb?pvs=4",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist/",
    "agent.js",
    "agent.d.ts",
    "containers/local.js",
    "containers/local.d.ts",
    "Dockerfile",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "bin": {
    "magnet-agent": "./dist/cli.js",
    "magnet-agent-server": "./dist/defaultAgentServer.js"
  },
  "scripts": {
    "format": "prettier --check .",
    "clean": "rm -rf dist && node ./scripts/clean-import-stubs.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepublish": "pnpm run build",
    "build": "pnpm clean && pnpm run write-version && tsc --project tsconfig.json && node ./scripts/add-import-stubs.js",
    "write-version": "node ./scripts/write-version.js && git add ./lib/version.ts",
    "start-agent-server": "node ./dist/defaultAgentServer.js"
  },
  "dependencies": {
    "@fastify/multipart": "^7.6.1",
    "@fastify/websocket": "^8.1.0",
    "@octokit/rest": "^19.0.11",
    "archiver": "^5.3.1",
    "axios": "^1.4.0",
    "commander": "^10.0.1",
    "decompress": "^4.2.1",
    "fastify": "^4.18.0",
    "form-data": "^4.0.0",
    "ignore": "^5.2.4",
    "indent-string": "^4.0.0",
    "kleur": "^4.1.5",
    "langchain": "^0.0.98",
    "posthog-node": "^3.1.1",
    "prompts": "^2.4.2",
    "uuid": "^9.0.0",
    "walkdir": "^0.4.1",
    "ws": "^8.13.0",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.3"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.2",
    "@types/decompress": "^4.2.4",
    "@types/node": "^20.3.1",
    "@types/prompts": "^2.4.4",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "openai": "^3.3.0",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hey-pal/magnet-agent.git"
  },
  "bugs": {
    "url": "https://github.com/hey-pal/magnet-agent/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./agent": {
      "types": "./dist/agent/index.d.ts",
      "import": "./dist/agent/index.js",
      "require": "./dist/agent/index.js"
    },
    "./containers/local": {
      "types": "./dist/containers/local.d.ts",
      "import": "./dist/containers/local.js",
      "require": "./dist/containers/local.js"
    },
    "./package.json": "./package.json"
  },
  "gitHead": "3b948c35343692810412c65128a5c58f739abfd5"
